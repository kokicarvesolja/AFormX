FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p; // tlak/gostota!
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0; //na zaƒçetku je 0, potem tekom kalkukacij naraste, dodani patchi fuselage, ailerons, elevator. Povzeto po raes datoteki. 

boundaryField
{
    movingWall
    {
        type            freestreamPressure;
        freestreamValue $internalField;
    }


    front
    {
        type            freestreamPressure;
        freestreamValue $internalField;
    }
    
    back 
    { 
	    type 		    freestreamPressure;
	    freestreamValue $internalField;
    }
	
	fuselage
    {
        type            zeroGradient;
    }

	ailerons
    {
        type            zeroGradient;
    }

	elevator
    {
        type            zeroGradient;
    }
}

// ************************************************************************* //
