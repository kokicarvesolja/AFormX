FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 33.5;

boundaryField
{
    
	movingWall
    {
        type            freestream;
        freestreamValue $internalField;
    }

    front
    {
        type            freestream;
        freestreamValue $internalField;
    }
  
    back
    {
	    type 		    freestream;
	    freestreamValue $internalField;
    }

    sfera1
    {
        type            omegaWallFunction;
        value           $internalField;
    }

    sfera2
    {
        type            omegaWallFunction;
        value           $internalField;
    }
}

// ************************************************************************* //
