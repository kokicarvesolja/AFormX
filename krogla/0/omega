FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 264.63;

boundaryField
{
    
	movingWall
    {
        type            freestream;
        freestreamValue uniform 264.63;
    }

    frontAndBack
    {
        type            freestream;
        freestreamValue uniform 264.63;
    }

    trup
    {
        type            omegaWallFunction;
        value           $internalField;
    }
}

