FoamFile
{
    version     2.0;
    format      binary;
    class       volScalarField;
    location    "0";
    object      S;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    inlet
    {
        type            fixedValue;
        value           uniform 0;
    }
    outlet
    {
        type            zeroGradient;
    }
    side
    {
        type            zeroGradient;
    }
    monkey 
    {
        type            zeroGradient;
    }
    monkeyBack 
    {
        type            fixedValue;
        value           uniform 1;
    }
}


